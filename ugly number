class Solution {
public:
    bool isUgly(int n) {
        if (n <= 0) return false;  // Ugly numbers must be positive
        while (n % 2 == 0) n /= 2;  // Divide by 2
        while (n % 3 == 0) n /= 3;  // Divide by 3
        while (n % 5 == 0) n /= 5;  // Divide by 5
        return n == 1;  // If n is reduced to 1, it means it only had 2, 3, and 5 as its prime factors
    }
};
